      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof
        dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    networks:
      default: null
    ports:
    - mode: ingress
      target: 80
      published: "5000"
      protocol: tcp
    tty: true
    volumes:
    - type: bind
      source: D:\GitHub\DockerCompose\api-users\api-users
      target: /app
      bind:
        create_host_path: true
    - type: bind
      source: D:\GitHub\DockerCompose\api-users
      target: /src
      bind:
        create_host_path: true
    - type: bind
      source: C:\Users\power\vsdbg\vs2017u5
      target: /remote_debugger
      bind:
        create_host_path: true
    - type: bind
      source: C:\Users\power\.nuget\packages\
      target: /root/.nuget/packages
      read_only: true
      bind:
        create_host_path: true
    - type: bind
      source: C:\Users\power\AppData\Roaming/ASP.NET/Https
      target: /root/.aspnet/https
      read_only: true
      bind:
        create_host_path: true
    - type: bind
      source: C:\Users\power\AppData\Roaming/Microsoft/UserSecrets
      target: /root/.microsoft/usersecrets
      read_only: true
      bind:
        create_host_path: true
  client:
    build:
      context: D:\GitHub\DockerCompose\api-users\users-client
      dockerfile: Dockerfile
    image: client
    networks:
      default: null
    ports:
    - mode: ingress
      target: 3000
      published: "5001"
      protocol: tcp
  proxy:
    image: nginx
    networks:
      default: null
    ports:
